<core:FragmentDefinition xmlns="sap.m"
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog title="{i18n>sessionTitle}" contentHeight="30rem" contentWidth="40rem" verticalScrolling="false">
        <buttons>
            <Button text="{i18n>refer}" press="onRefresh" type="Emphasized"/>
        </buttons>
        <IllustratedMessage 
        title = "{i18n>sessionExp}" description="{i18n>sessionQues}"
        illustrationSize = "Auto" illustrationType='tnt-SessionExpired' />
    </Dialog>
</core:FragmentDefinition>